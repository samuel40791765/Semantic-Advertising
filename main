import numpy as np
import cv2
from scipy.io import wavfile

cap = cv2.VideoCapture('dataset/Videos/data_test1_cmp.avi')
fs, data = wavfile.read('dataset/Videos/data_test1.wav')

while(cap.isOpened()):
    ret, frame = cap.read()

    frame_cvt = cv2.cvtColor(frame, cv2.COLOR_BGR2BGRA)

    cv2.imshow('video',frame_cvt)
    if cv2.waitKey(1) & 0xFF == ord('q'):
        break

cap.release()
cv2.destroyAllWindows()
